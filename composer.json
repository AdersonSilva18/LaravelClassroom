{
    "name": "adersonsilva/laravel-classroom",
    "description": "Pacote para integracao do Google Classroom com o Laravel",
    "type": "library",
    "license": "mit",
    "autoload": {
        "psr-4": {
            "AdersonSilva\\LaravelClassroom\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdersonSilva\\LaravelClassroom\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "AdersonSilva",
            "email": "75860265+AdersonSilva18@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0"
    },
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AdersonSilva\\LaravelClassroom\\Providers\\ClassroomServiceProvider"
            ]
        }
    }
}